{
    "compilerOptions": {
        "target": "ESNext",
        "module": "CommonJS",
        "outDir": "./dist",
        "rootDir": ".",
        "strict": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "types": ["node"],
        "skipLibCheck": true,
        "resolveJsonModule": true
    },
    "include": ["server.ts", "*.json"]
}
